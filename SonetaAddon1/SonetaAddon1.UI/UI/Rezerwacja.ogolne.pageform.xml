<?xml version="1.0" encoding="utf-8" ?>

<DataForm xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xmlns:xsd="http://www.w3.org/2001/XMLSchema"
          xmlns="http://www.enova.pl/schema/form.xsd"
          xsi:schemaLocation="http://www.enova.pl/schema/ http://www.enova.pl/schema/form.xsd"
          RightName="Page:OgolnePage"
          Priority="100000"
          DataType="SonetaAddon1.Rezerwacja, SonetaAddon1">
  <Page Name="OgolnePage" CaptionHtml="Rezerwacja" DataContext="{DataSource}">
	  <Group LabelWidth="12">
		  <Row>
			  <Field EditValue="{NrRezerwacji}" Width="6" />
			  <Field EditValue="{Data}" CaptionHtml="Data rezerwacji" LabelWidth="12" Width="14"/>
		  </Row>	
		  <Field EditValue="{Klient}" Width="38" />
		  <Field EditValue="{Lot}" Width="38" />
		  <Field EditValue="{CenaLotu}" LabelWidth="36" Width="14"/>
		  <Field EditValue="{Maszyna}" Width="38"/>
		  <Field EditValue="{CzyOplacona}" CaptionHtml="Czy zapłacona" LabelWidth="36" Width="14" />
	  </Group>
  </Page>
</DataForm>

